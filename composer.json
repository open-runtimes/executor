{
    "name": "open-runtimes/executor",
    "description": "Serverless runtimes executor for container based environments ⚡️",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Eldad Fux",
            "email": "eldad@appwrite.io"
        }
    ],
    "autoload": {
        "psr-4": {
            "OpenRuntimes\\Executor\\": "src/OpenRuntimes/Executor"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "repositories": [
        {
            "url": "https://github.com/appwrite/runtimes.git",
            "type": "git"
        }
    ],
    "require": {
        "php": ">=8.0.0",
        "ext-curl": "*",
        "ext-imagick": "*",
        "ext-mbstring": "*",
        "ext-json": "*",
        "ext-swoole": "*",
        "utopia-php/framework": "0.19.*",
        "utopia-php/logger": "0.1.*",
        "utopia-php/cli": "0.11.*",
        "utopia-php/registry": "0.5.*",
        "utopia-php/preloader": "0.2.*",
        "utopia-php/swoole": "0.3.*",
        "utopia-php/orchestration": "0.3.*",
        "appwrite/php-runtimes": "dev-refactor"
    },
    "require-dev": {
        "phpunit/phpunit": "9.5.10",
        "swoole/ide-helper": "4.8.5",
        "vimeo/psalm": "4.13.1"
    },
    "config": {
        "platform": {
            "php": "8.0"
        }
    }
}